                "4nand1:11,4nand1:9",
                "4nand1:8,4nand2:13",
                "4nand1:6,4nand2:1",
                "4nand2:11,4nand2:2",
                "4nand2:12,4nand2:3",
                "4nand2:11,4nand2:9",
                "bus:1,4nand2:10",
                "4nand2:8,4nand3:13",
                "bus:2,4nand3:1",
                "4nand3:2,4nand3:11",
                "4nand3:3,4nand3:12",
                "4nand3:11,bus:10"
