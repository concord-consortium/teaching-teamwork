{
    "name": "Inverter Ripple",
    "notes": "Shows animated ripple of chained inverters",
    "interface": {
        "showPinColors": true,
        "showBusColors": true,
        "showBusLabels": true,
        "allowAutoWiring": true,
        "showPinouts": true,
        "showProbe": "edit",
        "showInputAutoToggles": false,
        "showGlobalIOWires": false
    },
    "busSize": 7,
    "busInputSize": 1,
    "busOutputSize": 6,
    "boards": [{
        "logicChipDrawer": {
            "chips": {
                "7404": {
                    "max": 1
                }
            }
        },
        "localInputSize": 1,
        "localOutputSize": 1,
        "autoWiring": {
            "chips": {
                "chip": {
                    "type": "7404",
                    "x": 300,
                    "y": 150
                }
            },
            "wires": [
                "input:1, chip:13",
                "chip:12, chip:11",
                "chip:10, chip:9",
                "chip:8, chip:1",
                "chip:2, chip:3",
                "chip:4, chip:5",
                "chip:6, output:1"
            ]
        }
    }]
}
